<%= form_for @team do |f| %>

      <div class="field">
        <%= f.label :teamname %>
        <%= f.text_field :teamname, :size => 30 %>
      </div>
    
    <div class="field">
    <%= f.label :image %>
    <%= f.text_field :image, :size => 30 %>
    </div>
    
      <div class="field">
        <%= f.label :explanation  %>
        <%= f.text_area :explanation, :size => "30x2" %>
      </div>
    
      <div class="field">
        <%= f.label :country %>
        <%= f.text_field :country, :size => 30 %>
      </div>
    
      <%= f.submit "編集する" %>
    <% end %>